flute_whistle = "C:/Users/Aston Turney/Downloads/fluteredlight.wav"
bass_heavy = "C:/Users/Aston Turney/Downloads/basslight.wav"
lyricsred = "C:/Users/Aston Turney/Downloads/BabyRedlightlyrics.wav"
lyricsbop = "C:/Users/Aston Turney/Downloads/babyboplyrics.wav"
jetson = "C:/Users/Aston Turney/Downloads/jetson.wav"


live_loop :flute do
  use_bpm 165
  with_synth :hollow do
    with_fx :reverb do
      with_fx :level do
        
        sample flute_whistle
        sleep 21.875
        
      end
    end
  end
end

sleep 8

use_bpm 165
live_loop :bass do
  2. times do
    with_fx :reverb do
      sample bass_heavy, amp: 2.6
      sleep 21.875
    end
  end
  stop
end

sleep 0.6

with_fx :reverb do
  live_loop :lyr do
    2. times do
      sample lyricsred, amp: 1.78
      sleep 21.875
    end
    stop
  end
end
sleep 34.88

sample jetson, amp: 2.2

sleep 8.2555

live_loop :bop do
  with_fx :reverb do
    p1 = [chord(:G2,"minor7"),
          chord(:E2,"major7"),
          chord(:A2,"minor7"),
          chord(:E2,"m9"),
          chord(:G2,"m9"),
          chord(:C2,"minor7"),
          chord(:Fs2,"major7"),
          chord(:D2,"m11")
          ]
    
    cc = chord(:G2,"minor")
    2. times do
      sample lyricsbop, amp: 1.78
      sleep 22.5
    end
  end
end
